<div class="container">
    <h1>Vaše projekty</h1>
    <table>
        <tr>
            <th>id</th>
            <th>Jméno</th>
            <th>Autor</th>
        </tr>
        {{#each}}
            <tr>
                <td>{{id}}</td>
                <td>
                    {{#link-to 'project' this}}
                        {{name}}
                    {{/link-to}}
                </td>
                <td>{{author}}</td>
            </tr>
        {{/each}}
    </table>
</div>